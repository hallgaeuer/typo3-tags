    "type": "typo3-cms-extension",
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^12.4 || ^13.0"
    },
    "autoload": {
        "psr-4": {
            "Hallgaeuer\\Tags\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "tags"
        }
    }
}